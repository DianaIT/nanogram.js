{
  "name": "nanogram.js",
  "version": "1.0.0",
  "description": "",
  "main": "dist/lib/nanogram.js",
  "module": "dist/lib/nanogram.esm.js",
  "browser": "dist/lib/nanogram.umd.js",
  "unpkg": "dist/lib/nanogram.umd.js",
  "jsdelivr": "dist/lib/nanogram.umd.js",
  "typings": "dist/types/nanogram.d.ts",
  "files": [
    "dist/*",
    "test/*"
  ],
  "scripts": {
    "build": "run-s prebuild build:cjs other:rollup other:typedoc",
    "build:cjs": "tsc --module commonjs",
    "dev": "rollup -c @config/rollup.config.ts -w",
    "lint": "run-s eslint:write prettier:write",
    "other:commit": "git-cz",
    "other:eslint:write": "eslint --ext .js,.ts {src,test}/* --fix",
    "other:prettier:write": "prettier --write \"{src,test}/**/*.{js,ts}\"",
    "other:rollup": "rollup -c @config/rollup.config.ts",
    "other:typedoc": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "prebuild": "rimraf dist",
    "start": "npm run dev",
    "test": "jest --config jest.config.js --coverage",
    "test:watch": "jest --config jest.config.js --coverage --watch",
    "ec": "editorconfig-checker --config .ecrc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webistomin/nanogram.js.git"
  },
  "author": "webistomin",
  "email": "webistomin@gmail.com",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "bugs": {
    "url": "https://github.com/webistomin/nanogram.js/issues"
  },
  "homepage": "https://github.com/webistomin/nanogram.js#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@ls-lint/ls-lint": "^1.8.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.2",
    "@types/eslint": "^6.8.0",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/jest": "^25.2.1",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/node": "^13.11.1",
    "@types/prettier": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^25.3.0",
    "editorconfig-checker": "^3.0.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.3",
    "lodash.camelcase": "^4.3.0",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.6.0",
    "rollup-plugin-banner2": "^1.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "rollup-plugin-visualizer": "^4.0.4",
    "scriptlint": "^2.1.1",
    "ts-jest": "^25.3.1",
    "tslib": "^1.11.1",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3"
  },
  "dependencies": {}
}
